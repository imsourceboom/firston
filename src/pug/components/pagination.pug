- var p = 2;
ul.pagination
    li.first
        a(href="")
            img(src="/assets/images/symbols/chevrons-left.svg", alt="")
    li.prev
        a(href="")
            img(src="/assets/images/symbols/chevron-left.svg", alt="")
    li.counts.active
        a(href="") 1
    while p < 6
        li.counts
            a(href="")=p++
    li.next
        a(href="")
            img(src="/assets/images/symbols/chevron-right.svg", alt="")
    li.last
        a(href="")
            img(src="/assets/images/symbols/chevrons-right.svg", alt="")